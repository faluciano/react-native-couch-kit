{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@couch-kit/core": ["../core/src"],
      "expo-file-system": ["./node_modules/expo-file-system/build/index.d.ts"],
      "expo-file-system/next": [
        "./node_modules/expo-file-system/build/index.d.ts"
      ]
    }
  },
  "include": ["src/**/*", "../core/src/**/*"]
}
